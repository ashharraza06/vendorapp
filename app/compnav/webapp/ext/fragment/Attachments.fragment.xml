<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:macros="sap.fe.macros"
displayBlock="true"
xmlns:mvc="sap.ui.core.mvc"
	xmlns:upload="sap.m.upload">
	<VBox id= "11" >
	<!-- <Button id = "upload" core:require="{ handler: 'compnav/ext/fragment/Attachments'}" text="Attachment" press="handler.onPress" /> -->
						<upload:UploadSet
					
					id="uploadSet"
					core:require="{ handler: 'compnav/ext/fragment/Attachments'}"
					instantUpload="false"
					uploadEnabled="true"
					uploadButtonInvisible= "true"
					showIcons="true"
					mode="None"
					afterItemAdded="handler.onAfterItemAdded"
					uploadCompleted="handler.onUploadCompleted"
					items="{
								path: '/files',
								parameters: {
									$orderby: 'createdAt desc'
								},
								templateShareable: false}"
				>
					<upload:toolbar>
					</upload:toolbar>
					<upload:items>
						<upload:UploadSetItem
						id="ddd"
							fileName="{fileName}"
							mediaType="{mediaType}"
							url="{url}"
							thumbnailUrl="{
								path: 'mediaType',
								formatter: 'handler.formatThumbnailUrl'
							}"
							enabledEdit="false"
							openPressed="handler.onOpenPressed"
							removePressed="handler.onRemovePressed"
							visibleEdit="false"
							visibleRemove="false"
							enabledRemove="false"
						>
							<upload:attributes>
								<ObjectAttribute
								id="dd"
									title="Uploaded By"
									text="{createdBy}"
									active="false"
								/>
								<ObjectAttribute
								id="dd22"
									title="Uploaded on"
									text="{createdAt}"
									active="false"
								/>
								<ObjectAttribute
								id="dddw2"

									title="File Type"
									text="{mediaType}"
									active="false"
								/>
								<ObjectAttribute
								id="dd22a"
									title="File Size"
									text="{size}"
									active="false"
								/>
							</upload:attributes>
						</upload:UploadSetItem>
					</upload:items>
				</upload:UploadSet>
	</VBox>
</core:FragmentDefinition>
